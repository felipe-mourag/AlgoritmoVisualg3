algoritmo "CaixaEletrônico"
var
  valor: real
  nota: vetor[1..7] de real
  c : vetor[1..7] de inteiro
  i: inteiro
inicio
 c[1] <-200
 c[2] <-100
 c[3] <-50
 c[4] <-20
 c[5] <-10
 c[6] <-5
 c[7] <-2
  escreval("Digite um valor: ")
  leia(valor)
    escreval("Para o valor de: ",valor)
  para i <- 1 ate 7 faça
     nota[i] <- valor/c[i]
     nota[i] <- nota[i]%c[i]
     valor <- valor%c[i]
     escreval("É preciso de ",nota[i]," notas de ",c[i])
  fimpara
fimalgoritmo